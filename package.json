{
  "name": "redux-effects-fetch-fixture",
  "version": "0.0.11",
  "description": "Middleware for redux-effects-fetch to serve requests from a local fixture definition",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start --single-run",
    "lint": "eslint -c ./.eslintrc.yml --ext .js src",
    "prepublish": "npm run test && npm run lint && rm -rf lib && babel ./src --out-dir ./lib",
    "postpublish": "rm -rf lib && babel ./src --out-dir ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-boris/redux-effects-fetch-fixture.git"
  },
  "keywords": [
    "redux",
    "effects",
    "side-effects"
  ],
  "author": "Team Boris",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/team-boris/redux-effects-fetch-fixture/issues"
  },
  "homepage": "https://github.com/team-boris/redux-effects-fetch-fixture#readme",
  "dependencies": {
    "lodash": "^4.6.1",
    "redux-effects-fetch": "^0.5.5",
    "url-parse": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^3.6.0",
    "eslint-plugin-import": "^1.16.0",
    "jasmine": "^2.4.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "webpack": "^1.12.14"
  }
